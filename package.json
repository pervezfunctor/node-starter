    "esbuild": "0.13.15",
    "esbuild-register": "3.2.0",
    "eslint": "8.3.0",
    "lodash": "4.17.21",
    "nodemon": "2.0.15",
    "prettier": "2.5.0",
    "superstruct": "0.15.3",
    "tap": "15.1.5",
    "typescript": "4.5.2"
  },
